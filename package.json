{
  "name": "template-next-express-gql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "prod:server": "NODE_ENV=production ts-node --files  --transpile-only ./server/index.ts",
    "dev:server": "ts-node --files  --transpile-only ./server/index.ts",
    "start": "npm run prod:server",
    "dev": "./node_modules/nodemon/bin/nodemon.js --watch 'server/**/*.ts' --exec 'npm run dev:server'",
    "nodemon": "./node_modules/nodemon/bin/nodemon.js",
    "next:build": "next build",
    "prod:build": "NODE_OPTIONS=--max_old_space_size=1024 next build",
    "lint:ts": "tslint -c tslint.json '**.ts*'",
    "pretty": "prettier --write client/**/*.ts* server/**/*.ts",
    "precommit": "lint-staged",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "prod:db:migrate": "NODE_ENV=production ./node_modules/.bin/sequelize db:migrate",
    "prod:db:migrate:undo": "NODE_ENV=production ./node_modules/.bin/sequelize db:migrate:undo",
    "gql:gen": "./node_modules/.bin/graphql-codegen --config ./generated-gql/codegen.yml"
  },
  "author": "",
  "license": "ISC",

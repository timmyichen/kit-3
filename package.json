    "camelize": "^1.0.0",
    "classnames": "^2.2.6",
    "connect-session-sequelize": "^6.0.0",
    "dataloader": "^1.4.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "express-router-async": "^0.3.0",
    "express-session": "^1.16.1",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
